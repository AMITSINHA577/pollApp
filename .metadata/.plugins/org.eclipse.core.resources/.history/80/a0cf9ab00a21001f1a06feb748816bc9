package com.pollapp.entity;

@Entity
@Table(name = "polls")
public class Poll {
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    @Column(nullable = false)
    private String question;

    @ElementCollection
    @Column(nullable = false)
    private List<String> options;

    @Column(nullable = false)
    private String createdBy;
